{
  "name": "@the-monthly-sum/eslint-config",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./eslint.config.js"
  },
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules dist .tanstack .nitro .output",
    "format": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --write",
    "format:check": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --check",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@the-monthly-sum/prettier-config",
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.32.0",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@tanstack/eslint-plugin-router": "^1.130.12",
    "@the-monthly-sum/prettier-config": "workspace:*",
    "@the-monthly-sum/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "^9.32.0",
    "eslint-plugin-react-compiler": "latest",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "typescript": "catalog:",
    "typescript-eslint": "^8.39.0"
  }
}
