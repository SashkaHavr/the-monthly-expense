{
  "name": "@the-monthly-expense/utils",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "exports": {
    "./schemas": "./src/schemas.ts",
    "./categories": "./src/categories.ts",
    "./try-catch": "./src/try-catch.ts"
  },
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules dist .tanstack .nitro .output",
    "format": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --write",
    "format:check": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --check",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@the-monthly-expense/prettier-config",
  "dependencies": {
    "zod": "catalog:"
  },
  "devDependencies": {
    "@the-monthly-expense/eslint-config": "workspace:*",
    "@the-monthly-expense/prettier-config": "workspace:*",
    "@the-monthly-expense/typescript-config": "workspace:*",
    "@trpc/server": "catalog:trpc",
    "@types/bun": "catalog:",
    "typescript": "catalog:"
  }
}
